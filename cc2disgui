local function DEQH_fake_script() -- launch.LocalScript 
	local script = Instance.new('LocalScript', launch)

	local destroyshit = script.Parent.Parent.Parent.Parent.Disclaimer
	local warnfirecd = script.Parent.Parent.firecd
	local warngamepass = script.Parent.Parent.gamepasswarn
	local password = script.Parent.Parent.password
	local buttontext = script.Parent.Parent.launch
	local function run()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/XGerman009/scripts/main/cc2infogui'),true))()
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if KRNL_LOADED or secure_load or is_sirhurt_closure or pebc_execute or IS_VIVA_LOADED or isvm or IS_COCO_LOADED then
			print("Your exploit not supports. Shutdowning...")
			print("Your exploit not supports. Shutdowning...")
			print("Your exploit not supports. Shutdowning...")
			print("Your exploit not supports. Shutdowning...")
			print("Your exploit not supports. Shutdowning...")
			warngamepass:Destroy()
			warnfirecd.Text = "Sorry! Your exploit not supports script."
		end
		if is_synapse_function or syn then
			print("Your exploit supports script. Running...")
			warngamepass:Destroy()
			warnfirecd.Text = "Your exploit support script! Running..."
			wait(3)
			destroyshit:Destroy()
			run()
		end
	end)	
end
coroutine.wrap(DEQH_fake_script)()
local function KQHTJDO_fake_script() -- shit.LocalScript 
	local script = Instance.new('LocalScript', shit)

	local bye1 = script.Parent.Parent.Parent.Parent.Disclaimer
	local button1 = script.Parent.Parent.shit
	
	button1.Activated:Connect(function()
		bye1.Parent:Destroy()
	end)
end
coroutine.wrap(KQHTJDO_fake_script)()
local function VTGL_fake_script() -- _1.Move 
	local script = Instance.new('LocalScript', _1)

	local UIS = game:GetService('UserInputService')
	
	local frame = script.Parent
	
	
	
	local dragToggle = nil
	
	local dragSpeed = 0.50
	
	local dragStart = nil
	
	local startPos = nil
	
	
	
	local function updateInput(input)
	
		local delta = input.Position - dragStart
	
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
	
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	
	end
	
	
	
	frame.InputBegan:Connect(function(input)
	
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
	
			dragToggle = true
	
			dragStart = input.Position
	
			startPos = frame.Position
	
			input.Changed:Connect(function()
	
				if input.UserInputState == Enum.UserInputState.End then
	
					dragToggle = false
	
				end
	
			end)
	
		end
	
	end)
	
	
	
	UIS.InputChanged:Connect(function(input)
	
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	
			if dragToggle then
	
				updateInput(input)
	
			end
	
		end
	
	end)
end
coroutine.wrap(VTGL_fake_script)()
local function AFKGAEB_fake_script() -- _1.Rainbowshit 
	local script = Instance.new('LocalScript', _1)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
		script.Parent.UIStroke.Color = Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.01
	end
end
coroutine.wrap(AFKGAEB_fake_script)()
